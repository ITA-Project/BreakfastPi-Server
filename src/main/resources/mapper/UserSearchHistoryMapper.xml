<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ita.domain.mapper.UserSearchHistoryMapper">
  <resultMap id="BaseResultMap" type="com.ita.domain.entity.UserSearchHistory">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="user_id" jdbcType="INTEGER" property="userId" />
    <result column="shop_id" jdbcType="INTEGER" property="shopId" />
    <result column="search_key" jdbcType="VARCHAR" property="searchKey" />
  </resultMap>
  <insert id="insert" parameterType="com.ita.domain.entity.UserSearchHistory">
    insert into user_search_history (id, user_id, shop_id,
      search_key, create_time, update_time)
    values (#{id,jdbcType=INTEGER}, #{userId,jdbcType=INTEGER}, #{shopId,jdbcType=INTEGER},
      #{searchKey,jdbcType=CHAR}, now(), now())
  </insert>
</mapper>